package com.sps.javaAbstraction;
//functional interface example 2
@FunctionalInterface
interface work{
	public void info(String n);
}
public class FuntionalInterfaceExample2 {
	public static void main(String[] args) {
		work obj=new work() {
			public void info(String n) {
				System.out.println("Hello "+n);
			}
		};
		obj.info("Shivam");
	}
}
