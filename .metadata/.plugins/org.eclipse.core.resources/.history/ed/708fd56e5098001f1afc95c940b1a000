package com.sps.generics;


public class GenericsExample2 {
    public static void main(String[] args) {
    	class Demo<T> {
    	    T v;
    	    public Demo(T v) {
    	        this.v = v;  
    	    }
    	    public T getValue() {
    	        return v;
    	    }
    	}
    	
        Demo<String> obj = new Demo<>("Komal");
        System.out.println(obj.getValue());
    }
}

