package com.sps.generics;

class Demo<T> {
    T v;

    public Demo(T v) {
        this.v = v;  // Corrected syntax
    }

    public T getValue() {
        return v;
    }
}

public class GenericsExample3 {
    public static void main(String[] args) {
        Demo<String> obj = new Demo<>("suresh");
        System.out.println(obj.getValue());
    }
}
