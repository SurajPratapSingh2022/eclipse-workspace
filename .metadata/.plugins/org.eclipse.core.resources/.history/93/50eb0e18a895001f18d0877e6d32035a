package com.sps.javaNetworking;
//socket server ka code
import java.io.*;
import java.net.*;
public class SocketServer {
	public static void main(String[] args) {
		ServerSocket obj=new ServerSocket(1212);
		Socket s=obj.accept();
		DataInputStream obj1=new DataInputStream(s.getPort());
		String str=(string) obj1.readUTF();
		System.out.println(str);
	}
}
